{
  "name": "What Is My IP",
  "version": "2.1",
  "background":  {  "page":"Background.html"},
  "description": "What Is My IP, by NiceWebTools.com",
  "browser_action": {
      "default_title": "What Is My IP",
      "default_icon": "1.ico"
  },
  "icons": {
    "16": "1.ico",
    "48": "1.ico",
    "128": "1.ico"
  },
